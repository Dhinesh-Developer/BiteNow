  	<servlet-class>com.kumar.Servlets.RestaurantMenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlets</servlet-name>
  	<url-pattern>/LoginServlets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeRestaurant</servlet-name>
  	<url-pattern>/HomeRestaurant</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RestaurantMenuServlet</servlet-name>
  	<url-pattern>/RestaurantMenuServlet</url-pattern>
