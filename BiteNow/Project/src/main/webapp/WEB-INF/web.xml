  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.kumar.Servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeRestaurant</servlet-name>
