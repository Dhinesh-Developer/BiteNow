<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.kumar.Servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.kumar.Servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlets</servlet-name>
  	<display-name>LoginServlets</display-name>
  	<description></description>
  	<servlet-class>com.kumar.Servlets.LoginServlets</servlet-class>
  </servlet>
  <servlet>
